[alias]
    it = //src/main:odict-java

[httpserver]
    port = 0

[maven_repositories]
    central = https://repo1.maven.org/maven2

[intellij]
    jdk_name = Java SDK 1.8
    jdk_type = JavaSDK
    java_module_sdk_type = JavaSDK
    java_module_sdk_name = 1.8
    module_group_name = hpc

[project]
    ide = intellij
    glob_handler = watchman
    ignore = .git

[java]
    src_roots = java, scala
    source_level = 8
    target_level = 8

[download]
    in_build = true